x=[1949 1954 1959 1964 1969 1974 1979 1984 1989 1994];
y=[5.4 6.0 6.7 7.0 8.1 9.1 9.8 10.3 11.3 11.8];
%线性拟合
a=polyfit(x,y,1);
x1=1949:10:1994;
y1=a(2)+a(1)*x1;
%指数拟合
b=polyfit(x,log(y),1);
y2=exp(b(2))*exp(b(1)*x1);
plot(x,y,'*');
hold on
plot(x1,y1,'--r');
hold on
plot(x1,y2,'-k');
legend('原曲线','模型一曲线','模型二曲线')
